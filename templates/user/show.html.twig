{% extends 'admin.html.twig' %}


{% block table %}
<div class="breadcome-area mg-b-30 small-dn">
       <div class="container-fluid">

<div class="row">
               <div class="col-lg-12">
                   <div class="breadcome-list shadow-reset">
                       <div class="row">
                           <div class="col-lg-9">
                               <div class="breadcome-heading">

                               </div>
                           </div>
                           <div class="col-lg-3">
                               <ul class="breadcome-menu">
                                   <li><a href="{{ path('user_index')}}">User</a> <span class="bread-slash">/</span>
                                   </li>
                                   <li><span class="bread-blod">Show</span>
                                   </li>
                               </ul>
                           </div>
                       </div>

                   </div>
                   
               </div>

           </div>

           <div class="row">
               <div class="col-lg-12">
                   <div class="breadcome-list shadow-reset">
                       <div class="row">
                           <div class="col-lg-9">
                            <br>  <a href="{{ path('user_edit', {'id': user.id}) }}">Edit</a><br>

                               <div class="breadcome-heading">

  <table class="table">
        <tbody>
            <tr>
                <th>Username</th>
                <td>{{ user.username }}</td>
            </tr>
            <tr>
                <th>UsernameCanonical</th>
                <td>{{ user.usernameCanonical }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>EmailCanonical</th>
                <td>{{ user.emailCanonical }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ user.enabled ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Salt</th>
                <td>{{ user.salt }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user.password }}</td>
            </tr>
            <tr>
                <th>LastLogin</th>
                <td>{{ user.lastLogin ? user.lastLogin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ConfirmationToken</th>
                <td>{{ user.confirmationToken }}</td>
            </tr>
            <tr>
                <th>PasswordRequestedAt</th>
                <td>{{ user.passwordRequestedAt ? user.passwordRequestedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ user.roles ? user.roles|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ user.id }}</td>
            </tr>
            <tr>
                <th>Dn</th>
                <td>{{ user.dn }}</td>
            </tr>
            <tr>
                <th>Post</th>
                <td>{{ user.post }}</td>
            </tr>
            <tr>
                <th>DateEmbauche</th>
                <td>{{ user.dateEmbauche ? user.dateEmbauche|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    {{ include('user/_delete_form.html.twig') }}



                               </div>
                           </div>
                          
                       </div>

             <a href="{{ path('user_index') }}">back to list</a>

                   </div>
                   
               </div>

           </div>

           

       </div>
   </div>

  
   
{% endblock %}
