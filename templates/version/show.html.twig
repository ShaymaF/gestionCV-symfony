{% extends 'base.html.twig' %}

{% block title %}Version{% endblock %}

{% block body %}
    <h1>Version</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ version.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ version.name }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ version.dateCreation ? version.dateCreation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ version.path }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('version_index') }}">back to list</a>

    <a href="{{ path('version_edit', {'id': version.id}) }}">edit</a>

    {{ include('version/_delete_form.html.twig') }}
{% endblock %}
