<body>
{% if app.user %}
<!--Pricing Section-->
<section  class="pdf" style="font-size: 15px;margin-top: 10%;">

      <div class="row" style="padding-left: 1%;padding-right: 1%;" >
         <div class="main_pricing roomy-100"  >
            <div class="col-md-12 col-sm-12"  >
                <div class=" pricing_item"  >
                  <div  p-top-12 style=" text-align: center;color: white;">
              
                     <h1>{{user.username}}</h1>
                     <p>
                        <h3>{{user.post}} <br>
                        {{ "now"|date("Y")- user.dateEmbauche |date(" Y")}} ans d’expérience
                    </h3>
                     </p>
                     <br><br>
                     <!--   <div class="impress_btns m-top-30">
                        <a href="" class="btn btn-primary m-top-20">export .pdf</a>
                        <a href="" class="btn text-white bg-mega m-top-20">SHOW CV</a>
                        <a href="" class="btn btn-primary  m-top-20">export .doc</a>
                        
                        </div>-->
                  </div>
                  <div class="pricing_body bg-white p-top-150 p-bottom-60">
                     <div class="row col-md-12 col-sm-12">
                     <br>
                        <div class="col-md-6">
                           <p><b><a class="m-top-30"  ><i class="fa fa-user" style="font-size:20px"> À propos de moi</i></a></b></p><br>
                           {% for a in about %}
                           <p style="text-align:justify;"> <span style="color: #1E8BC3;"> &bull;</span> {{a.text}}
                           </p>
                           {% endfor %}
                           
                        </div>
                        <div class="col-md-6">
                           <p><b><a class="m-top-30"  ><i class="fa fa-thumbs-up" style="font-size:20px"> Compétences</i></a></b></p><br>
                           {% for c in competence %}
                         <p style="text-align:justify;">  <span style="color: #1E8BC3;"> &bull; </span>{{c.competenceField}}</p>
                           {% endfor %}
                            
                        </div>                        

                     </div>
                  <div class="col-md-12">
                   <hr> </div>
                     
                     <div class="service_item col-md-12">
                     
                        <div class="col-md-6">
                        </div>
                        <div class="col-md-6">
                        
                        </div>
                     </div>
                     <div class="service_item col-md-12">
                     
                        <div class="col-md-6">
                           <div class="service_item"
                           <p><b><a class="m-top-30"  ><i class="fa fa-globe" style="font-size:20px"> Langues</i></a></b></p><br>
                             </b></a></p>
                              {% for lg in langues %}
                              <a> <b style="color: #1E8BC3;">&#10148; {{lg.langue}} </b></a> 
                              <a><b></b></a> {{ lg.niveau }} / 10   
                              <br>
                              {% endfor %}
                           </div>
                        </div>
                        <div class="col-md-6">
                           <div class="service_item">
                           <p><b><a class="m-top-30"  ><i class="fa fa-heart" style="font-size:20px"> Loisirs</i></a></b></p><br>
                              {% for p in loisirs %}
                            <p style="text-align:justify;">  {{p.titre}} </p>
                             
                              {% endfor %}
                           </div>
                        </div>
                     </div>
                       <div class="col-md-12">
                   <hr> </div>
                     <div class="col-md-12">
                      <div class="col-md-6">
                           <div class="service_item">
                           <p><b><a class="m-top-30"><i class="fa fa-graduation-cap" style="font-size:20px"> Formation</i></a></b></p><br>

                              {% for f in formation %}
                           <p>  <b> <i class="fa fa-calendar" style="color: #765a9c;font-size: 15px;"> {{f.datedebut|date("d |m |y")}} - {{f.datefin|date("d |m |y")     }}   </i> </b> </p>

                              <a> <b>  Dipôlme </a> </b><p style="text-align:justify">{{f.titre}}</p>
                              <a> <b style="text-align:justify;color: #1E8BC3;">  Etablissement </a> </b><p style="text-align:justify">{{f.Etablissement}}</p>  
                              <br>
                              {% endfor %}
                           </div>
                        </div>
                        <div class="col-md-6">
                           <div class="service_item">
                           <p><b><a class="m-top-30"  ><i class="fa fa-laptop" style="font-size:20px"> Connaissances</i></a></b></p><br>
                              {% for cn in connaissances %}
                              {% if cn.titre=='choice1' %} <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Conception  </b></a>{{cn.technologies}}{% endif %}
                              {% if cn.titre=='choice2' %}  <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; BDD et Cache  </b></a>{{cn.technologies}}{% endif %}
                              {% if cn.titre=='choice3' %} <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Développement  </b></a>{{cn.technologies}} {% endif %}
                              {% if cn.titre=='choice4' %}  <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Méthodologies  </b></a>{{cn.technologies}}{% endif %}
                              {% if cn.titre=='choice5' %}  <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Outils de développement  </b></a>{{cn.technologies}}{% endif %}
                              {% if cn.titre=='choice6' %}  <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Intégratrion continue  </b></a>{{cn.technologies}}{% endif %}
                              {% if cn.titre=='choice7' %}  <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Serveurs d application  </b></a>{{cn.technologies}}{% endif %}
                              {% if cn.titre=='choice8' %}  <a> <b style="text-align:justify;color: #1E8BC3;">&#10148; Systémes </b></a>{{cn.technologies}}{% endif %}
                              <br>
                              {% endfor %}
                           </div>
                        </div>
                       
                     </div>
                       <div class="col-md-12">
                   <hr> </div>
                     <div class="col-md-12">
                    
                        <div class="service_item">
                           <p><b><a class="" style="font-size: 26px;"><i class="fa fa-black-tie" style="font-size: 20px;"> Parcours Professionnel</i> </a></b></p><br><br>
                           {% for pr in parcours %}	

                            <div class="col-md-3">
                          <p>  <b> <i class="fa fa-calendar" style="color: #765a9c;font-size: 15px;"> {{pr.dateDebut|date("m | y")}} -{{pr.datefin|date(" m | y")}}</i> </b> </p>
                           <p style="text-align:justify;">   {{pr.post }} <br><br> {{ pr.emplacement}} </p> 

                          </div>
                          
                          <div  class="col-md-9">
                          
                           <p style="text-align:justify;font-size: 15px;"><b style="color: #1E8BC3;"> Description Projet </b><br>{{ pr.description}} </p>
                           <p style="text-align:justify;font-size: 15px;"><b style="color: #1E8BC3;"> Mission  </b><br>{{ pr.mission}} </p>
                           </div>                    
                            <div class="col-md-12">
                     <div class="col-md-4" ></div> <hr>  <div class="col-md-8"></div>
                      </div>

                           {% endfor %}
                          
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
         <!-- End off col-md-4 -->
      </div>
   </div>
   <!--End off row-->
   <!--End off container -->
</section>
<!--End off Pricing section -->

